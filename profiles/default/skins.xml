<?xml version="1.0"?>

<object name="portal_skins"
        meta_type="Plone Skins Tool"
        allow_any="False"
        cookie_persistence="False"
        request_varname="plone_skin">

 <object name="uwoshmusicrecruiting_images"
         meta_type="Filesystem Directory View"
         directory="UWOshMusicRecruiting/skins/uwoshmusicrecruiting_images"/>
 <object name="uwoshmusicrecruiting_styles"
         meta_type="Filesystem Directory View"
         directory="UWOshMusicRecruiting/skins/uwoshmusicrecruiting_styles"/>
 <object name="uwoshmusicrecruiting_templates"
         meta_type="Filesystem Directory View"
         directory="UWOshMusicRecruiting/skins/uwoshmusicrecruiting_templates"/>
 
 <skin-path name="Plone Default">
  <layer insert-after="custom" 
         name="uwoshmusicrecruiting_images"/>
  <layer insert-after="custom" 
         name="uwoshmusicrecruiting_styles"/>
  <layer insert-after="custom" 
         name="uwoshmusicrecruiting_templates"/>
 </skin-path>

</object>